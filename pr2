import sys

N = 16

task1 = (17 - 5 * 8) / 12 + 25 * N
print(task1)

task2 = 7.77 * (27.4 - N + 34.3) / 5 + 2 ** 3
print(task2)

task3_first = (54 + 32.8) ** N
task3_second = 753 - 3.1 * task3_first + 77.7 / 6 - N
print(task3_second)

task4 = "Моє прізвище Кастрнець"
print(task4)

task5 = "Моє ім'я Ігор"

task6 = "Україна"
print('\n'.join(task6))

task7 = "Львів" * 5
res = len(task7) * 2
print(task7, res)
